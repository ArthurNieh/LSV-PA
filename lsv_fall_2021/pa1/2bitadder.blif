.model 4bitSerialAdder
.inputs a0 b0 a1 b1
.outputs s0 s1 c1
.subckt fullAdder a=a0  b=b0  cin=0  s=s0 cout=c0
.subckt fullAdder a=a1  b=b1  cin=c0 s=s1 cout=c1
.end

.model fullAdder
.inputs a b cin
.outputs s cout
.names a b cin s
001 1
010 1
100 1
111 1
.names a b cin cout
11- 1
1-1 1
-11 1
.end
